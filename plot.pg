#!/usr/bin/gnuplot
reset
set terminal png

set xlabel "Time Elapsed"
set ylabel "Index Covered"
set title "Execution Time Plot"

set key reverse Left outside
set grid
set style data linespoints

set xrange [0:100000]
set yrange [0:1000]

set output "plot_ll_recursion.png"
plot "ll_recursion.data" using 1:2 title "Data"

set output "plot_ll_loop.png"
plot "ll_loop.data" using 1:2 title "Data

